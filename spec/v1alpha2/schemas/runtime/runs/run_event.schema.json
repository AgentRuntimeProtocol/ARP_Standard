{
  "title": "RunEvent",
  "type": "object",
  "additionalProperties": false,
  "required": ["run_id", "seq", "type", "time"],
  "properties": {
    "run_id": {
      "type": "string"
    },
    "seq": {
      "type": "integer",
      "minimum": 0
    },
    "type": {
      "type": "string",
      "enum": ["token", "step", "tool_call", "tool_result", "final", "error"]
    },
    "time": {
      "type": "string",
      "format": "date-time"
    },
    "data": {
      "type": "object",
      "additionalProperties": true
    },
    "extensions": {
      "$ref": "../../common/extensions.schema.json"
    }
  }
}

